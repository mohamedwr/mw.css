          // Start Collapse
          
          a(href="#coll" id="coll" class="link-blue" style="font-size: 30px; margin-bottom: 20px;") Collapse
          br
          br
          div.h2
            div(class="collapse collapse-b h6" style="display: block;")
              button(class="btn btn-blue" onclick="collapse('collapse-1');") Show /Hide
              div(class="collapse collapse-w" data-id="collapse-1") This is white Collapse
              hr
              | #{'<button class="btn btn-blue" onclick="collapse(\'collapse-1\')">Show / Hide</button>'}
              br
              br
              | #{'<div class="collapse collapse-b" data-id="collapse-1">This is white Collapse</div>'}
              
            p.h5 You can cutomize collapse color (Black, White)
              div(class="collapse collapse-w h6" style="display: block;")
                button(class="btn btn-blue" onclick="collapse('collapse-2');") Show /Hide
                div(class="collapse collapse-b" data-id="collapse-2") This is black Collapse
                hr
                | #{'<div class="collapse collapse-b" data-id="collapse-2">This is black Collapse</div>'}
          
