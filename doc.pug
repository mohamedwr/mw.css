                  img(src="http://placehold.it/286x180")
                  div.content
                    h3 Hello
                    div.desc This is a portait card
                  br
                  button(class="btn btn-blue") Button
                hr
                | #{'<div class="card-portait card-portait-b">'}
          
          hr
          div.h2 Landscape
            div(class="collapse collapse-b h6" style="display: block;")
              div(class="card-landscape card-landscape-w")
                img(src="http://placehold.it/286x180")
                div(class="content clearfix")
                  h3 Hello
                  div.desc This is a landscape card
                  br
                  button(class="btn btn-blue") Button
              hr
              | #{'<div class="card-landscape card-landscape-w">'}
              br
